{
    "name": "laravel7/infusionsoft-php-sdk",
    "description": "PHP SDK for the Infusionsoft",
    "keywords": [
        "infusionsoft",
        "sdk"
    ],
    "homepage": "https://developer.infusionsoft.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Infusionsoft",
            "homepage": "https://developer.infusionsoft.com"
        }
    ],
    "require": {
        "php": "7.*",
        "guzzlehttp/guzzle": "*",
        "lstrojny/fxmlrpc": "^0.14",
        "psr/log": "~1.0",
        "php-http/guzzle6-adapter": "*",
        "laminas/laminas-diactoros": "^2.3",
        "php-http/message": "*",
        "php-http/curl-client": "*"
    },
    "require-dev": {
        "mockery/mockery": "*",
        "phpunit/phpunit": "*",
        "doctrine/instantiator": "^1.0",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-0": {
            "Infusionsoft": ["src", "tests"]
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infusionsoft\\FrameworkSupport\\Laravel\\InfusionsoftServiceProvider"
            ]
        }
    }
}
